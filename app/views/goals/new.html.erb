<h1>目標登録</h1>

<%= form_for(@goal_new) do |f| %>
 
    <span>タイトル</span><br>
    <%= f.text_field :title %><br>
 
    <span>内容</span><br>
    <%= f.text_field :body %><br>
    
    <span>目標設定時間</span><br>
    <%= f.number_field :set_time, step: '0.1', class:'input_form' %>
 
    <%= f.submit '登録する' %>
 
<% end %>